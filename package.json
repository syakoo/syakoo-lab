{
  "name": "syakoo-lab",
  "type": "module",
  "version": "6.0.0",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "lint": "eslint --ext .js,.ts,.tsx,.astro src",
    "lint:fix": "eslint --ext .js,.ts,.tsx,.astro --fix src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint --edit ${1} && pnpm exec emoji-prefix-commit",
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,astro,cjs,mjs}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,astro,cjs,mjs,css}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  },
  "dependencies": {
    "@astrojs/react": "^3.0.2",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/recipes": "^0.5.0",
    "astro": "^3.1.0",
    "probe-image-size": "^7.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-essentials": "^7.4.2",
    "@storybook/addon-interactions": "^7.4.2",
    "@storybook/addon-links": "^7.4.2",
    "@storybook/addon-viewport": "^7.4.5",
    "@storybook/blocks": "^7.4.2",
    "@storybook/manager-api": "^7.4.2",
    "@storybook/react": "^7.4.2",
    "@storybook/react-vite": "^7.4.2",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/theming": "^7.4.2",
    "@syakoo/emoji-prefix-commit": "github:syakoo/emoji-prefix-commit",
    "@types/probe-image-size": "^7.2.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "eslint": "^8.49.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.13",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prop-types": "^15.8.1",
    "simple-git-hooks": "^2.9.0",
    "storybook": "^7.4.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
