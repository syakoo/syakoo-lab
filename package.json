{
  "name": "syakoo-lab",
  "type": "module",
  "version": "6.0.0",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "run-p dev:*",
    "dev:local": "next dev",
    "dev:mock-server": "pnpm dlx tsx ./scripts/mock-server.ts",
    "build": "run-s build:prebuild build:next",
    "build:next": "next build",
    "build:prebuild": "pnpm dlx tsx ./scripts/prebuild.ts",
    "start": "next start",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:clear": "jest --clearCache",
    "test:update": "jest --updateSnapshot",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "pnpm run lint --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "scaffdog generate"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint --edit ${1} && pnpm exec emoji-prefix-commit",
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,cjs,mjs}": [
      "eslint --fix",
      "jest --findRelatedTests --passWithNoTests"
    ],
    "*.{js,ts,tsx,cjs,mjs,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource-variable/fira-code": "^5.0.15",
    "@fontsource-variable/noto-sans-jp": "^5.0.16",
    "@fontsource/roboto": "^5.0.8",
    "@mdx-js/mdx": "^2.3.0",
    "@mswjs/http-middleware": "^0.9.1",
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/next-plugin": "^2.3.2",
    "@vanilla-extract/recipes": "^0.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "jsdom": "^22.1.0",
    "mermaid": "^10.6.1",
    "next": "^14.0.2",
    "probe-image-size": "^7.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-katex": "^6.0.3",
    "rehype-pretty-code": "^0.10.2",
    "remark-math": "^5.1.1",
    "ress": "^5.0.2",
    "sharp": "^0.32.6",
    "shiki": "^0.14.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@next/eslint-plugin-next": "^14.0.2",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-viewport": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@syakoo/emoji-prefix-commit": "github:syakoo/emoji-prefix-commit",
    "@testing-library/react": "^14.1.0",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/gtag.js": "^0.0.18",
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^21.1.5",
    "@types/probe-image-size": "^7.2.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vanilla-extract/jest-transform": "^1.1.1",
    "@vanilla-extract/vite-plugin": "^3.9.1",
    "@vanilla-extract/webpack-plugin": "^2.3.1",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "msw": "^2.0.6",
    "msw-storybook-addon": "2.0.0--canary.122.b3ed3b1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "scaffdog": "^3.0.0",
    "simple-git-hooks": "^2.9.0",
    "storybook": "^7.5.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "undici": "^5.27.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
