{
  "name": "syakoo-lab",
  "type": "module",
  "version": "6.1.0",
  "engines": {
    "node": ">=22.21.0",
    "pnpm": ">=10.22.0"
  },
  "scripts": {
    "prepare": "lefthook install",
    "dev": "run-p dev:*",
    "dev:local": "next dev",
    "build": "pnpm run build:next",
    "build:next": "next build",
    "build:optimize-image": "tsx --env-file=.env.local --tsconfig ./tsconfig.json ./scripts/optimize-image.ts",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:clear": "vitest run --reporter=verbose --clearCache",
    "test:update": "vitest run -u",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "pnpm exec http-server storybook-static -a localhost -p 6006 -s",
    "storybook:test": "test-storybook --url http://localhost:6006",
    "storybook:test:ci": "concurrently -k -s first \"pnpm storybook:serve\" \"wait-on http://localhost:6006 && pnpm storybook:test\"",
    "generate": "scaffdog generate"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@t3-oss/env-nextjs": "^0.13.0",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.21.2",
    "glob": "^11.1.0",
    "gray-matter": "^4.0.3",
    "mermaid": "^11.0.0",
    "next": "^15.5.10",
    "probe-image-size": "^7.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-katex": "^7.0.1",
    "rehype-pretty-code": "^0.14.0",
    "remark-math": "^6.0.0",
    "sharp": "^0.34.0",
    "shiki": "^3.9.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "ts-pattern": "^5.7.1",
    "zod": "^3.24.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "core-js-pure",
      "esbuild",
      "lefthook",
      "sharp",
      "unrs-resolver"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@next/env": "^14.2.21",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-links": "^10.2.1",
    "@storybook/nextjs": "^10.2.1",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/gtag.js": "^0.0.20",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@types/probe-image-size": "^7.2.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/ui": "^3.2.4",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.2.1",
    "devmoji": "^2.3.0",
    "http-server": "^14.1.1",
    "jsdom": "^22.1.0",
    "lefthook": "^1.10.0",
    "npm-run-all2": "^5.0.0",
    "playwright": "^1.55.0",
    "scaffdog": "^3.0.0",
    "storybook": "^10.2.1",
    "tsx": "^4.20.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  },
  "packageManager": "pnpm@10.30.1"
}
