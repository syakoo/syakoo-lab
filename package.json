{
  "name": "syakoo-lab",
  "type": "module",
  "version": "6.0.0",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:clear": "jest --clearCache",
    "test:update": "jest --updateSnapshot",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "pnpm run lint --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:component": "hygen component new"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint --edit ${1} && pnpm exec emoji-prefix-commit",
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,cjs,mjs}": [
      "eslint --fix",
      "jest --findRelatedTests"
    ],
    "*.{js,ts,tsx,cjs,mjs,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource-variable/fira-code": "^5.0.15",
    "@fontsource-variable/noto-sans-jp": "^5.0.16",
    "@fontsource/roboto": "^5.0.8",
    "@mdx-js/mdx": "^2.3.0",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/next-plugin": "^2.3.1",
    "@vanilla-extract/recipes": "^0.5.0",
    "date-fns": "^2.30.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "jsdom": "^22.1.0",
    "mermaid": "^10.6.0",
    "next": "^14.0.1",
    "probe-image-size": "^7.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-katex": "^6.0.3",
    "rehype-pretty-code": "^0.10.2",
    "remark-math": "^5.1.1",
    "ress": "^5.0.2",
    "shiki": "^0.14.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@next/eslint-plugin-next": "^14.0.1",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addon-viewport": "^7.5.2",
    "@storybook/blocks": "^7.5.2",
    "@storybook/manager-api": "^7.5.2",
    "@storybook/nextjs": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/react-vite": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.2",
    "@syakoo/emoji-prefix-commit": "github:syakoo/emoji-prefix-commit",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.7",
    "@types/jsdom": "^21.1.4",
    "@types/probe-image-size": "^7.2.2",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vanilla-extract/jest-transform": "^1.1.1",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "@vanilla-extract/webpack-plugin": "^2.3.1",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "hygen": "^6.2.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "simple-git-hooks": "^2.9.0",
    "storybook": "^7.5.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
